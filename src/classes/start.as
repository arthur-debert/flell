import flell.Shell;var s : Shell = new Shell();s.init(stage);//"ls -l . "var commands : Array = [						"echo hello shell",						"ls -l -r  ",						"pwd ",						"cd /root1/box1",						"cd /badmovie/box1",						"pwd "						];for each (var raw :String in commands){	execute(raw);}function execute(c : String){	trace(c , "->", s.executeRaw(c) + "\n\n");}var completes : Array = [    "eh", "ech", "l", "p"];for each (raw in completes){	getCompletion(raw);}function getCompletion(c : String){	trace(c , "->", s.getCompletionsForCommand(c) );}var pathCompletes : Array = [    "/ro", "/root1/in", "/root1/box"];for each (raw in pathCompletes){	getCompletionsForPath(raw);}function getCompletionsForPath(c : String){	trace(c , "->", s.getCompletionsForPath(c) );}